{
  "name": "@shoutem/react-web-ui",
  "version": "0.15.2",
  "description": "React UI kit for Shoutem extensions",
  "main": "lib/index.js",
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "npm run build-dist && npm run build-lib",
    "clean-dist": "rimraf ./dist/*",
    "clean-lib": "rimraf ./lib/*",
    "build-dist": "npm run clean-dist && webpack --config ./bin/webpack.config.js",
    "build-lib": "npm run clean-lib && babel src --out-dir lib --copy-files",
    "release": "npm run build && npm publish --access public --tag latest",
    "release-rc": "npm run build && npm publish --access public --tag rc",
    "lint": "eslint src/**/*.{js,jsx} && eslint test/**/*.js"
  },
  "author": "Shoutem",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/shoutem/react-web-ui"
  },
  "homepage": "https://github.com/shoutem/react-web-ui#readme",
  "dependencies": {
    "auto-bind": "^4.0.0",
    "bootstrap-sass": "^3.3.6",
    "classnames": "^2.2.6",
    "emoji-mart": "^3.0.1",
    "lodash": "^4.17.19",
    "moment": "^2.24.0",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-bootstrap": "^0.32.4",
    "react-datetime": "^2.16.3",
    "react-debounce-input": "^3.2.2",
    "react-dnd": "^2.1.4",
    "react-dnd-html5-backend": "^2.1.2",
    "react-dom": "^16.12.0",
    "react-dropzone": "^3.7.4",
    "react-select": "^1.0.0-rc.5",
    "suneditor-react": "^2.16.3"
  },
  "devDependencies": {
    "@shoutem/eslint-config-react": "^1.0.1",
    "babel-cli": "^6.5.1",
    "babel-core": "^6.8.0",
    "babel-eslint": "^10.1.0",
    "babel-plugin-transform-react-jsx": "^6.8.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "css-loader": "^0.26.1",
    "cssnano": "^3.9.1",
    "eslint": "^6.8.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jsx-a11y": "^1.5.5",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^5.2.2",
    "extract-text-webpack-plugin": "^2.0.0-beta.4",
    "file-loader": "^0.9.0",
    "node-sass": "^4.1.1",
    "postcss-loader": "^1.2.1",
    "prettier": "^1.19.1",
    "rimraf": "^2.5.4",
    "sass-loader": "^4.1.1",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^2.1.0-beta.19"
  }
}
